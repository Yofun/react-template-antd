{
  "name": "practice-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "prepare": "husky install",
    "start": "craco start --config scripts/dev.config.js",
    "build": "craco build --config scripts/prod.config.js",
    "test": "craco test --config scripts/dev.config.js",
    "eject": "craco eject --config scripts/dev.config.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "antd": "^4.23.0",
    "axios": "^0.27.2",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@craco/craco": "^6.4.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.184",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "craco-less": "^2.0.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-scripts": "5.0.1",
    "sass": "^1.52.3",
    "typescript": "*",
    "web-vitals": "^2.1.4",
    "webpackbar": "^5.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": 1
    }
  },
  "lint-staged": {
    ".{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
